{
  "name": "stackoverflow-lite",
  "version": "1.0.0",
  "description": "StackOverflow-lite is a platform where people can as questions about challenges and get answers. They can also provide answers to other people questions.",
  "main": "app.js",
  "scripts": {
    "lint": "eslint server/",
    "start": "babel-node server/app.js",
    "dev-start": "nodemon --exec babel-node server/app.js",
    "test": "mocha server/**/*.js --require babel-core/register",
    "coverage": "babel-node ./node_modules/istanbul cover _mocha tests/index.js",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidshare/StackOverflow-lite.git"
  },
  "author": "David Essien",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidshare/StackOverflow-lite/issues"
  },
  "homepage": "https://github.com/davidshare/StackOverflow-lite#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "express": "^4.16.3",
    "winston": "^3.0.0"
  }
}
